class my_music():
    def __init__(self):
        self.tracks = []
        
    def store_artist_song_and_name(self, artist_name, song_name):
        self.tracks.append({"Artist": artist_name, "Song": song_name})
        
    def get_tracks(self):
        get_track_info = ""
        for track in self.tracks:
            get_track_info += f"Artist: {track['Artist']}, Song: {track['Song']}\n"
        return get_track_info
        
        
music = my_music()
music.store_artist_song_and_name("Oasis", "Dont Look Back In Anger")
music.store_artist_song_and_name("Oasis", "Importance of being idle")
music.store_artist_song_and_name("Test Artist", "Random Song")
result = music.get_tracks()
print(result)
