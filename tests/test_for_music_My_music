from lib.My_music import my_music

def test_for_storing_artist_song_and_name():
    music = my_music()
    music.store_artist_song_and_name("Oasis", "Wonderwall")
    assert len(music.tracks) == 1 
    
def test_for_get_tracks():
    music = my_music()
    music.store_artist_song_and_name("A1", "S1")
    music.store_artist_song_and_name("A2", "S2")
    get_track_info = music.get_tracks()
    result = "Artist: A1, Song: S1\nArtist: A2, Song: S2\n"
    assert get_track_info == result 
